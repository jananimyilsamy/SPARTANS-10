package datastructures;

import java.util.*;

public class swapnibbles {
	
	public static void main(String arg[]) 
	{ 
	    Scanner sc=new Scanner(System.in);
	    int size=sc.nextInt();
	    for(int i=0;i<size;i++)
	    {
	    int x=sc.nextInt();
	    
	    System.out.println(((x & 0x0F) << 4 | (x & 0xF0) >> 4));
	    }
	} 
	} 
	  
