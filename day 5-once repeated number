package datastructures;

import java.util.Scanner;

public class finduniquenumber {
	
		  public static int oncerepeatednumber(int[] array) {
		    int arr = 0;
		    for (int i : array) {
		      arr ^= i;
		    }
		    return arr;
		  }
		

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		int[] arr=new int[n];
		for(int i=0;i<n;i++)
		{
			arr[i]=sc.nextInt();
		}
		
		System.out.println(oncerepeatednumber(arr));

	}

}
